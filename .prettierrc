{
  "endOfLine": "lf",
  "semi": true,
  "singleQuote": false,
  "tabWidth": 2,
  "trailingComma": "es5",
  "plugins": [
    "@ianvs/prettier-plugin-sort-imports"
  ],
  "importOrder": [
    "^(react/(.*)$)|^(react$)",
    "",
    "^(react-native/(.*)$)|^(react-native$)",
    "",
    "^(expo-/(.*)$)|^(expo-$)",
    "",
    "<THIRD_PARTY_MODULES>",
    "",
    "^[@~]/screens/.*$",
    "",
    "^[@~]/components/.*$",
    "",
    "^[@~]/styles/.*$",
    "",
    "^[@~]/assets/fonts/.*$",
    "^[@~]/assets/images/.*$",
    "",
    "^[./]"
  ],
  "experimentalTernaries": true,
  "importOrderParserPlugins": ["jsx", "js", "decorators-legacy"]
}
